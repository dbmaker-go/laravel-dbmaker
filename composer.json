{
	"name": "dbmaker/laravel-odbc",
	"description": "DBMaker integration for Laravel framework ",
	"keywords": [
		"laravel",
		"DBMaker",
		"Database",
		"PDO",
		"ODBC"
	],
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/dbmaker-go/laravel-dbmaker",
	"authors": [
		{
			"name": "syscom",
			"email": "info@syscom.com.tw",
			"homepage": "http://www.dbmaker.com.tw",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.3",
		"illuminate/database": ">5.8",
		"laravel/framework": ">5.8",
		"ext-pdo": "*",
		"ext-odbc": "*"
	},
	"autoload": {
		"psr-4": {
			"DBMaker\\ODBC\\": "src/"
		},
		"classmap": [
			"src/"
		]
	} ,
	"extra": {
		"laravel": {
			"providers": [
				"DBMaker\\ODBC\\DBMakerServiceProvider"
			]
		}
	}


}
